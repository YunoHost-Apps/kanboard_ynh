{
  "name": "Kanboard",
  "id": "kanboard",
  "packaging_format": 1,
  "description": {
    "en": "Kanban project management software",
    "fr": "Logiciel de gestion de projet Kanban"
  },
  "version": "1.2.18~ynh2",
  "url": "https://kanboard.net/",
  "license": "MIT",
  "maintainer": {
    "name": "YunoHost Contributors",
    "email": "apps@yunohost.org"
  },
  "requirements": {
    "yunohost": ">= 4.1.7"
  },
  "previous_maintainers": [{
    "name": "mbugeia",
    "email": "maxime@max.privy.place"
  },
  {
    "name": "jibec",
    "email": "jean-baptiste@holcroft.fr"
  }],
  "multi_instance": true,
  "services": [
    "nginx",
    "php7.3-fpm",
    "mysql"
  ],
  "arguments": {
    "install": [
      {
        "name": "domain",
        "type": "domain",
        "example": "domain.org"
      },
      {
        "name": "path",
        "type": "path",
        "example": "/kanboard",
        "default": "/kanboard"
      },
      {
        "name": "admin",
        "type": "user",
        "example": "johndoe"
      },
      {
        "name": "is_public",
        "type": "boolean",
        "default": false
      }
    ]
  }
}
